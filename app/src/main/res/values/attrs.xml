<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinEntryEditText">
        <attr name="pinAnimationType" format="enum">
            <enum name="popIn" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr>
        <attr name="pinCharacterMask" format="string" />
        <attr name="pinRepeatedHint" format="string" />
        <attr name="pinLineStroke" format="dimension" />
        <attr name="pinLineStrokeSelected" format="dimension" />
        <attr name="pinCharacterSpacing" format="dimension" />
        <attr name="pinTextBottomPadding" format="dimension" />
        <attr name="pinLineColors" format="color" />
        <attr name="pinBackgroundDrawable" format="reference" />
        <attr name="pinBackgroundIsSquare" format="boolean" />
        <attr name="pinAnimateLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AccountCardView">
        <attr name="type" format="enum">
            <enum name="common" value="0" />
            <enum name="my_card" value="1" />
            <enum name="transfer_from_account" value="2" />
            <enum name="transfer_to_account" value="3" />
            <enum name="transfer_post" value="4" />
            <enum name="bill" value="5" />
            <enum name="fund" value="6" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MultiViewPager">
        <attr name="matchChildWidth" format="reference" />
        <attr name="android:maxWidth" />
        <attr name="android:maxHeight" />
    </declare-styleable>

    <declare-styleable name="BezierBackground">
        <attr name="viewType" format="enum">
            <enum name="account_list" value="0" />
            <enum name="landing" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomQRView">
        <attr name="size" format="enum">
            <enum name="full_screen" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParallaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoScrollableView" format="reference" />
        <attr name="umanoOverlay" format="boolean" />
        <attr name="umanoClipPanel" format="boolean" />
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="umanoScrollInterpolator" format="reference" />
    </declare-styleable>
    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragFromEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RecipientMenuView">
        <attr name="menuTitle" format="string" />
    </declare-styleable>
</resources>